<include src="/commons/navbar/navbar" />
<view class="after-navber">
    <form bindsubmit='doSearchLsGoods'>
    <view class='search-bar'>
      <view class='search-input'>
        <button class='ls-header_search' form-type='submit'></button>
        <input class='ls-header_keywords' name="header_keywords" placeholder='D210超显瘦牛仔短裤' placeholder-class='ls-header_keywords_placeholder'></input>
      </view>
      <view><button open-type="contact" class='ls-header_servicer'></button></view>
    </view>
    </form>
    <view class='ls-header_category'>
      <button class='ls-header_category_item {{0 == cat_mod.cat_id ? "active" : ""}}' data-cat-id="0" bindtap='changeCatTab'>全部</button>
      <block wx:for="{{cat_list_w}}">
        <button class='ls-header_category_item {{item.id == cat_mod.cat_id ? "active" : ""}}' data-cat-id="{{item.id}}" bindtap='changeCatTab'>{{item.name}}</button>
      </block>
    </view>

    <view class='category-tab' wx:if="{{cat_mod.show_category_tab}}">
      <view class='bd guess-like'>
        <navigator class='guess-like-goods' wx:for="{{cat_mod.goods_list}}" url='/pages/goods/goods?id={{item.id}}'>
          <image src='{{item.pic_url}}' mode='widthFix'></image>
          <view class='guess-like-name'>{{item.name}}</view>
          <view class='guess-like-price'>
            ￥{{item.price}}
            <view class='guess-like-sale'>已售{{item.virtual_sales}}件</view>
          </view>
        </navigator>
      </view>
    </view>

    <view class='main-tab' wx:else>

    <block wx:for="{{module_list}}" wx:for-item="item" wx:for-index="index">

        <block wx:if='{{item.name == "notice"}}'>

            <!--商城公告 开始-->
            <view class='notice flex-row' wx:if='{{notice}}' bindtap='showNotice' style='color:{{update_list.notice.color}};background-color:{{update_list.notice.bg_color}}'>
                <view class='flex-grow-0 flex-y-center notice-a'>
                    <image src='{{update_list.notice.icon}}' style='width:36rpx;height:36rpx;margin-right:24rpx;'></image>
                    <view>{{update_list.notice.name}}</view>
                </view>
                <view class='flex-grow-1 notice-box'>
                    <view class='notice-content'>
                        <view style='display: inline-block;'>{{notice}}</view>
                    </view>
                </view>
                <view class='flex-grow-0 flex-y-center notice-b'>
                    <image src='/images/icon-notice-jiantou.png' style='width:10rpx;height:18rpx;margin-left:32rpx;'></image>
                </view>
            </view>
            <view class='notice-modal flex-y-center flex-x-center {{show_notice?"":"hidden"}}'>
                <view style='margin-top:-200rpx;'>
                    <image class='notice-img' src='/images/icon-notice-title.png'></image>
                    <view class='w-100 notice-body'>
                        <scroll-view scroll-y style='color:#666;max-height:400rpx;'>
                            <text>{{notice}}</text>
                        </scroll-view>
                        <view class='flex-x-center'>
                            <view class='notice-btn flex-x-center flex-y-center' bindtap='closeNotice'>我知道了</view>
                        </view>
                    </view>
                </view>
            </view>
            <!--商城公告 结束-->
        </block>
        <!-- Banner 开始 -->
        <view wx:if="{{item.name=='banner'}}" style='margin-top:-10rpx;' class='banner-wrapper'>
            <swiper class="banner" autoplay="true" interval="5000" duration="300" circular="true" bindchange="changePointer">
                <block wx:for="{{banner_list}}">
                    <swiper-item>
                        <navigator bindtap="navigatorClick" data-url="{{item.page_url}}" data-open_type="{{item.open_type}}" url="{{item.page_url}}" open-type="{{item.open_type ? item.open_type : 'navigator'}}">
                            <view class="slide-title">{{item.title}}</view>
                            <image src="{{item.pic_url}}" class="slide-image" mode="aspectFill" />
                        </navigator>
                    </swiper-item>
                </block>
            </swiper>
            <view class='banner-pointer'>
              <image wx:for="{{banner_pointer.index_list}}" src='../../images/{{index == banner_pointer.current_index ? "dot-selected" : "dot-unselected"}}.png'></image>
            </view>
        </view>
        <!-- Banner 结束 -->
        
        <!-- 搜索栏 开始 -->
        <view wx:elif="{{item.name=='search'}}" style='margin-bottom:16rpx'>
            <view class="search-block">
                <navigator url="/pages/search/search">
                    <image src="/images/icon-search.png" />
                    <text>搜索</text>
                </navigator>
            </view>

           
        </view>
        <!-- 搜索栏 结束 -->

        

        <!-- 导航图标 开始 -->
        <view class='ls-nav-icon-list-wrapper' wx:elif="{{item.name=='nav'}}">
          <view class='ls-nav-icon-list-header' style='background-image:url(../../images/circle_bg.png)' ></view>
          <view class="ls-nav-icon-list" wx:if="{{nav_icon_list&&nav_icon_list.length>0}}">
              <view class="ls-nav-icon" wx:for="{{nav_icon_list}}">
                  <navigator bindtap="navigatorClick" data-url="{{item.url}}" data-open_type="{{item.open_type}}" url="{{item.url}}" open-type="{{item.open_type}}">
                      <image src="{{item.pic_url}}" mode='widthFix'></image>
                      <view>{{item.name}}</view>
                  </navigator>
              </view>
          </view>
        </view>
        <!-- 导航图标 结束 -->

        <!-- 专题 开始 -->
        <view wx:elif="{{item.name=='topic'}}">
            <navigator class="topic-bar" url="/pages/topic-list/topic-list">
                <view class="flex-row">
                    <view class="flex-grow-0 flex-y-center" wx:if='{{update_list.topic.count == 2}}'>
                        <image class="bar-icon" src="{{update_list.topic.logo_2}}"></image>
                    </view>
                    <view class="flex-grow-0 flex-y-center" wx:if='{{update_list.topic.count == 1}}'>
                        <image class="bar-icon-1" src="{{update_list.topic.logo_1}}"></image>
                    </view>
                    <view class="flex-grow-1">
                        <view style="height:72rpx;width:79%;position:absolute;z-index:2"></view>
                        <swiper wx:if="{{update_list.topic.count==1}}" style="height:32rpx;line-height:32rpx" vertical="true" display-multiple-items="1" autoplay="true" interval="4000" duration="500" circular="false">
                            <swiper-item wx:for="{{topic_list}}" class="flex-row">
                                <!-- <view class="topic-tag">热门</view> -->
                                <view class="flex-grow-0 flex-y-center">
                                    <image src='{{update_list.topic.heated}}' style='width:54rpx;height:28rpx;margin-right:10rpx;'></image>
                                </view>
                                <view class="flex-grow-1 topic-title fs-sm}">{{item.title}}</view>
                            </swiper-item>
                        </swiper>
                        <swiper wx:if="{{update_list.topic.count==2}}" style="height:68rpx;line-height:32rpx" vertical="true" display-multiple-items="1" autoplay="true" interval="4000" duration="500" circular="false">
                            <swiper-item wx:for="{{topic_list}}">
                                <view class="flex-row">
                                    <view class="flex-grow-0 flex-y-center">
                                        <image src='{{update_list.topic.heated}}' style='width:54rpx;height:28rpx;margin-right:10rpx;'></image>
                                    </view>
                                    <view class="flex-grow-1 topic-title fs-sm}">{{item[0].title}}</view>
                                </view>
                                <view class="flex-row" wx:if="{{item[1].title!==undefined}}">
                                    <view class="flex-grow-0 flex-y-center">
                                        <image src='{{update_list.topic.heated}}' style='width:54rpx;height:28rpx;margin-right:10rpx;'></image>
                                    </view>
                                    <view class="flex-grow-1 topic-title fs-sm">{{item[1].title}}</view>
                                </view>
                            </swiper-item>
                        </swiper>
                    </view>
                </view>
            </navigator>
        </view>
        <!-- 专题 结束 -->

        <!-- 优惠券 开始 -->
        <view wx:elif="{{item.name=='coupon'}}">
            <view class="coupon" wx:if='{{coupon_list.length>0}}'>
                <view class='coupon-title flex-y-center flex-x-center'>
                    <view class='flex-y-center title'>
                        <image src="/images/icon-coupon.png" />
                        <text>专享优惠券</text>
                    </view>
                    <navigator class="flex-y-center" bindtap="hideGetCoupon" url="/pages/coupon-list/coupon-list">
                        <text>更多</text>
                        <image src="/images/icon-jiantou-r.png" />
                    </navigator>
                </view>
                <scroll-view scroll-x="true" style="height: 162rpx">
                    <view class='coupon-list flex-row'>
                        <block wx:for='{{coupon_list}}' wx:for-item="item" wx:for-index="index">
                            <view class='coupon-one flex-grow-0' data-index='{{item.id}}' bindtap='receive'>
                                <image src='{{item.is_receive == 0 ?update_list.coupon.bg:update_list.coupon.bg_1}}'></image>
                                <view class='coupon-content flex-row'>
                                    <view class='flex-grow-0 coupon-left'>
                                        <view class='flex-x-center sub flex-y-center'>
                                            <block wx:if='{{item.discount_type == 2}}'>
                                                <view class="flex-y-bottom">
                                                    <text style='font-size: 9pt;line-height:1'>￥</text>
                                                    <text style='font-size:15pt;line-height:0.8'>{{item.sub_price}}
                                                    </text>
                                                </view>
                                            </block>
                                            <block wx:if='{{item.discount_type == 1}}'>
                                                <view class="flex-y-bottom">
                                                    <text style='font-size:22pt;line-height:0.8'>{{item.discount}}
                                                    </text>
                                                    <text style='line-height:1'>折</text>
                                                </view>
                                            </block>
                                        </view>
                                        <view class='flex-x-center flex-y-center min'>满{{item.min_price}}元可用</view>
                                    </view>
                                    <block wx:if='{{item.is_receive == 0}}'>
                                        <view class='flex-grow-1 coupon-right flex-x-center flex-y-center'>立即领取
                                        </view>
                                    </block>
                                    <block wx:if='{{item.is_receive == 1}}'>
                                        <view class='flex-grow-1 coupon-right flex-x-center flex-y-center'>已领取</view>
                                    </block>
                                </view>
                            </view>
                        </block>
                        <view class="coupon-one flex-grow-0" style="width: 1rpx !important;;opacity: 0 !important;overflow: hidden">1
                        </view>
                    </view>
                </scroll-view>
            </view>
        </view>
        <!-- 优惠券 结束 -->


        <!-- 全部分类 开始 -->
        <view wx:elif="{{item.name=='cat'}}">
            <block wx:for="{{cat_list}}" wx:for-index="cat_index" wx:for-item="cat">
                <view wx:if="{{cat.goods_list.length>0}}" style="background: #fff;margin-bottom: 10rpx; width:100%;overflow-x: hidden;">
                    <view class="title-bar flex-y-center flex-x-center">
                        <view class="title-line"></view>
                        <view class="title flex-y-center">
                            <image src="{{cat.pic_url}}" mode="aspectFill"></image>
                            <text>{{cat.name}}</text>
                        </view>
                        <view class="title-line"></view>
                        <navigator class="flex-y-center" bindtap="hideGetCoupon" url="/pages/list/list?cat_id={{cat.id}}">
                            <text>更多</text>
                            <image src="/images/icon-jiantou-r.png" />
                        </navigator>
                    </view>
                    <view class="goods-list flex-row goods-list-cols-{{cat_goods_cols}}">
                        <view class="flex-grow-0" wx:for="{{cat.goods_list}}" wx:for-index="goods_index" wx:for-item="goods">
                            <navigator class="goods-item" url="/pages/goods/goods?id={{goods.id}}">
                                <image src="{{goods.pic_url}}" mode="aspectFill" />
                                <text class="text-more-2 {{cat_goods_cols == 3?'fs-sm':''}}" style='padding:0 20rpx;height:2.8em;line-height:1.4;'>{{goods.name}}</text>
                                <text class="goods-price" wx:if='{{cat_goods_cols == 3}}'>￥{{goods.price}}</text>
                                <view class='flex-row' style='padding:10rpx 20rpx' wx:if='{{cat_goods_cols == 2}}'>
                                    <view class='flex-grow-1' style='color:#ff334b'>￥{{goods.price}}</view>
                                    <view class='fs-sm' style='color:#999;'>已售{{goods.sales}}</view>
                                </view>
                            </navigator>
                        </view>
                    </view>
                </view>
            </block>

        </view>
        <!-- 全部分类 结束 -->

        <!-- 拆分的分类 开始 -->
        <view wx:elif="{{item.name=='single_cat'}}">
            <block wx:for="{{cat_list}}" wx:for-index="cat_index" wx:for-item="cat">
                <block wx:if="{{item.cat_id==cat.id}}">
                    <view wx:if="{{cat.goods_list.length>0}}" style="background: #fff;margin-bottom: 10rpx;width:100%;overflow-x: hidden;">
                        <view class="title-bar flex-y-center flex-x-center">
                            <view class="title-line"></view>
                            <view class="title flex-y-center">
                                <image src="{{cat.pic_url}}" mode="aspectFill"></image>
                                <text>{{cat.name}}</text>
                            </view>
                            <view class="title-line"></view>
                            <navigator class="flex-y-center" bindtap="hideGetCoupon" url="/pages/list/list?cat_id={{cat.id}}">
                                <text>更多</text>
                                <image src="/images/icon-jiantou-r.png" />
                            </navigator>
                        </view>
                        <view class="goods-list flex-row goods-list-cols-{{cat_goods_cols}}">
                            <view class="flex-grow-0" wx:for="{{cat.goods_list}}" wx:for-index="goods_index" wx:for-item="goods">
                                <navigator class="goods-item" url="/pages/goods/goods?id={{goods.id}}">
                                    <image src="{{goods.pic_url}}" mode="aspectFill" />
                                    <text class="text-more-2 {{cat_goods_cols == 3?'fs-sm':''}}" style='padding:0 20rpx;height:2.8em;line-height:1.4;'>{{goods.name}}</text>
                                    <text class="goods-price" wx:if='{{cat_goods_cols == 3}}'>￥{{goods.price}}</text>
                                    <view class='flex-row' style='padding:10rpx 20rpx' wx:if='{{cat_goods_cols == 2}}'>
                                        <view class='flex-grow-1' style='color:#ff334b'>￥{{goods.price}}</view>
                                        <view class='fs-sm' style='color:#999;'>已售{{goods.sales}}</view>
                                    </view>
                                </navigator>
                            </view>
                        </view>
                    </view>
                </block>
            </block>
        </view>
        <!-- 拆分的分类 结束 -->


        <!-- 秒杀板块 开始 -->
        <block wx:elif="{{item.name=='miaosha'}}">
            <view class="user-block">
                <view class="flex-row miaosha-header" style="padding: 0 20rpx;height: 80rpx;background: #fff;margin-bottom: 4rpx">
                    <view class="flex-grow-1 flex-row flex-y-center">
                        <image src="/images/icon-miaosha.png" style="width: 34rpx;height: 34rpx;margin-right: 12rpx"></image>
                        <view style="font-size: 12pt;color: #ff8b57;margin-right: 16rpx">整点秒杀</view>
                        <view style="margin-right: 24rpx">{{miaosha.name}}</view>

                        <view class="flex-y-center miaosha-time-item" style="width: 32rpx;height: 34rpx">
                            <image src="/images/icon-time-bg.png" style="width: 100%;height: 100%"></image>
                            <view class="flex-y-center flex-x-center">{{miaosha.times.h?miaosha.times.h:'--'}}</view>
                        </view>

                        <image src="/images/icon-time-split.png" class="miaosha-item-split" style="width: 20rpx;height: 34rpx"></image>

                        <view class="flex-y-center miaosha-time-item" style="width: 32rpx;height: 34rpx">
                            <image src="/images/icon-time-bg.png" style="width: 100%;height: 100%"></image>
                            <view class="flex-y-center flex-x-center">{{miaosha.times.m?miaosha.times.m:'--'}}</view>
                        </view>

                        <image src="/images/icon-time-split.png" class="miaosha-item-split" style="width: 20rpx;height: 34rpx"></image>

                        <view class="flex-y-center miaosha-time-item" style="width: 32rpx;height: 34rpx">
                            <image src="/images/icon-time-bg.png" style="width: 100%;height: 100%"></image>
                            <view class="flex-y-center flex-x-center">{{miaosha.times.s?miaosha.times.s:'--'}}</view>
                        </view>

                    </view>
                    <navigator url="/pages/miaosha/miaosha" class="flex-grow-0 flex-row flex-y-center">
                        <view style="margin-right: 14rpx">更多</view>
                        <image src="/images/icon-jiantou-r.png" style="width: 12rpx;height: 22rpx"></image>
                    </navigator>
                </view>
                <scroll-view scroll-x="true" style="background: #fff;padding: 16rpx 8rpx">
                    <view class="flex-row miaosha-goods-list">
                        <navigator class="flex-grow-0 miaosha-goods" style="background: #fff;" wx:for="{{miaosha.goods_list}}" url="/pages/miaosha/details/details?id={{item.id}}">
                            <image src="{{item.pic}}"></image>
                            <view class='text-more-2'>{{item.name}}</view>
                            <view style="color: #ff4544;line-height: 1;text-overflow: ellipsis;overflow: hidden;white-space: nowrap" class="flex-row flex-y-center flex-x-center">
                                <view>￥</view>
                                <view style='font-size:13pt'>{{item.miaosha_price}}</view>
                            </view>
                            <view style="color: #aaa;line-height: 1;text-overflow: ellipsis;overflow: hidden;white-space: nowrap;text-decoration: line-through;font-size: 9pt" class="flex-y-center flex-x-center">
                                <view>￥</view>
                                <view>{{item.price}}</view>
                            </view>
                        </navigator>
                    </view>
                </scroll-view>
            </view>
        </block>
        <!-- 秒杀板块 结束 -->

        <!-- 拼团板块 开始 -->
        <view class="user-block pintuan-block" wx:elif="{{item.name=='pintuan'}}" style='margin-bottom:16rpx;'>
            <view class="flex-row flex-y-center pintuan-header" style="height: 80rpx;padding: 0 20rpx;">
                <view class="flex-grow-1 flex-y-bottom">
                    <image src="/images/icon-pintuan-text.png" style="width: 88rpx;height: 40rpx;margin-right: 20rpx"></image>
                    <view style="color: #aaa;font-size: 9pt;line-height: 1.2">限量拼团，每日必逛</view>
                </view>
                <view class="flex-grow-0">
                    <navigator url="/pages/pt/index/index" class="flex-y-center">
                        <view style="margin-right: 14rpx">更多</view>
                        <image src="/images/icon-jiantou-r.png" style="width: 12rpx;height: 22rpx"></image>
                    </navigator>
                </view>
            </view>
            <block wx:for="{{pintuan.goods_list}}">
              <!-- <view class='list-item flex-row' data-gid='{{item.id}}' bindtap='goDetails'> -->
              <navigator url="/pages/pt/details/details?gid={{item.id}}" open-type="navigate" class='list-item flex-row'>
                  <view class='flex-grow-0 corver-pic'>
                      <image lazy-load="true" src='{{item.pic}}'></image>
                  </view>
                  <view class='flex-grow-1 flex-col'>
                      <view class='flex-grow-1 goods-title'>
                          {{item.name}}
                      </view>
                      <view class='flex-grow-0 flex-row'>
                          <view class='group-item' wx:if='{{item.groupList[0]}}'>
                              <view wx:for='{{item.groupList}}' wx:for-item="group">
                                  <image src='{{group.avatar_url}}'></image>
                              </view>
                          </view>
                          <view class='goods-sales'>
                              已团 {{item.virtual_sales}} 件
                          </view>
                      </view>
                      <view class='flex-grow-0 flex-row flex-y-bottom'>
                          <view class='flex-grow-0 price flex-row flex-y-bottom'>
                              <view class='unit flex-grow-0 flex-y-bottom'>
                                  ¥
                              </view>
                              <view class='flex-grow-1 flex-y-bottom'>{{item.price}}</view>
                          </view>
                          <view class='flex-grow-1 old-price flex-y-bottom'>
                              ¥ {{item.original_price}}
                          </view>
                          <view class='flex-grow-0 group-btn flex-x-center flex-y-center'>
                              去开团
                          </view>
                      </view>
                  </view>
              </navigator>
          </block>
        </view>
        <!-- 拼团板块 结束 -->

        <view class='module huanqiu' wx:elif="{{item.name=='huanqiu'}}">
          <view class='hd'>
            热卖推荐
            <image src='../../images/more.png'></image>
          </view>
          <scroll-view scroll-x class='bd huanqiu-bd' scroll-with-animation="true">
            <navigator class='huanqiu-bd-goods hot' wx:for="{{huanqiu.hot.list}}" url='/pages/goods/goods?id={{item.id}}'>
              <image src='{{item.pic_url}}' mode='widthFix'></image>
              <view class='huanqiu-goods-name'>{{item.name}}</view>
              <view class='huanqiu-goods-price'>￥{{item.price}}</view>
            </navigator>
          </scroll-view>
        </view>

        <!-- 预约板块 开始 -->
        <block wx:elif="{{item.name=='yuyue'}}">
            <view class="user-block">
                <view class="flex-row miaosha-header" style="padding: 0 20rpx;height: 80rpx;background: #fff;margin-bottom: 4rpx">
                    <view class="flex-grow-1 flex-row flex-y-center">
                        <image src="/images/icon-yuyue.png" style="width: 34rpx;height: 34rpx;margin-right: 12rpx"></image>
                        <view style="font-size: 12pt;color: #ff8b57;margin-right: 16rpx">预约</view>
                    </view>
                    <navigator url="/pages/book/index/index" class="flex-grow-0 flex-row flex-y-center">
                        <view style="margin-right: 14rpx">更多</view>
                        <image src="/images/icon-jiantou-r.png" style="width: 12rpx;height: 22rpx"></image>
                    </navigator>
                </view>
                <scroll-view scroll-x="true" style="background: #fff;padding: 16rpx 8rpx">
                    <view class="flex-row miaosha-goods-list">
                        <block wx:for="{{yuyue}}">
                            <navigator class="flex-grow-0 miaosha-goods" style="" url="/pages/book/details/details?id={{item.id}}">
                                <image src="{{item.cover_pic}}"></image>
                                <view class='text-more-2'>{{item.name}}</view>
                                <view style="color: #ff4544;line-height: 1;text-overflow: ellipsis;overflow: hidden;white-space: nowrap" class="flex-row flex-y-center">
                                    <block wx:if='{{item.price > 0}}'>
                                        <view>￥</view>
                                        <view style='font-size:13pt'>{{item.price}}</view>
                                    </block>
                                    <block wx:else>
                                        <view>免费</view>
                                    </block>
                                </view>
                            </navigator>
                        </block>
                    </view>
                </scroll-view>
            </view>
        </block>
        <!-- 预约板块 结束 -->

        <!-- 视频板块 开始 -->
        <block wx:elif='{{item.name == "video"}}'>
            <block wx:for="{{update_list.video}}" wx:for-item="video" wx:if="{{item.video_id == video.name}}">
                <view class='user-block' bindtap='play' data-index='{{video.name}}'>
                    <view wx:if="{{play == video.name}}">
                        <video class='video' style='width:750rpx;height:400rpx;' autoplay='true' src='{{video.url}}' controls='true' loop='true' bindfullscreenchange='fullscreenchange'></video>
                    </view>
